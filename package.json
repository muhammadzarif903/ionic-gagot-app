{
  "name": "GagotApp",
  "version": "0.0.1",
  "author": "Maxim Livshitz",
  "description": "An Ionic GagotApp project",
  "homepage": "",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "hmr": "ng serve --hmr",
    "build": "ng build",
    "test": "ng test",
    "lint": "eslint . --ext .ts",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./ --output-path dist && electron .",
    "build:electron": "ng build --base-href ./ --output-path dist",
    "build:electron:prod": "ng build --prod --base-href ./ --output-path dist",
    "build_win": "electron-packager . --platform=win32 --asar --icon=gagotapp.ico --overwrite --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/resources --ignore=/platforms --ignore=/src",
    "build:test": "ng test --watch false --browsers=ChromeHeadlessCI",
    "dist": "electron-builder",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "clean": "rimraf ./node_modules && npm i",
    "sync:android": "npx jetify && npx cap sync android",
    "sync:ios": "npx cap sync ios",
    "android_dev": "ionic cap run android --livereload --external --source-map=false",
    "android": "npm run build && npx cap copy android && npx cap open android",
    "ios": "npm run build && npx cap copy ios && npx cap open ios",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
